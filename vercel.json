{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "VITE_SUPABASE_PROJECT_ID": "@vite-supabase-project-id",
    "VITE_SUPABASE_URL": "@vite-supabase-url",
    "VITE_SUPABASE_PUBLISHABLE_KEY": "@vite-supabase-publishable-key"
  }
}
